<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>FirstPge</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>dashboardController</servlet-name>
        <servlet-class>cm.lal.controllers.dashboardController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>cm.lal.controllers.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>indexServlet</servlet-name>
        <servlet-class>cm.lal.controllers.indexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LecturerServlet</servlet-name>
        <servlet-class>cm.lal.controllers.LecturerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>cm.lal.controllers.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FirstPge</servlet-name>
        <servlet-class>cm.lal.controllers.FirstPge</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MoveToServlet</servlet-name>
        <servlet-class>cm.lal.controllers.MoveToServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dashboardController</servlet-name>
        <url-pattern>/dashboardController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>indexServlet</servlet-name>
        <url-pattern>/indexServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LecturerServlet</servlet-name>
        <url-pattern>/LecturerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FirstPge</servlet-name>
        <url-pattern>/FirstPge</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoveToServlet</servlet-name>
        <url-pattern>/MoveToServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>10</session-timeout>
    </session-config>
</web-app>
